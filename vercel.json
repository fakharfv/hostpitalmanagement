{
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static"
      },
      {
        "src": "server.php",
        "use": "@vercel/php"
      },
    {
      "src": "Dockerfile",
      "use": "@vercel/docker"
    }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.php"
      }
    ]
  }
